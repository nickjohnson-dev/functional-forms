{
  "name": "functional-forms",
  "version": "1.0.0-1",
  "description": "Dynamic declarative forms for React",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist --ignore test.js",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "prebuild": "npm run clean -s",
    "prerelease": "npm run lint -s && npm run testonce -s && npm run build -s",
    "release": "npm publish",
    "start": "webpack-dev-server --hot --inline --config ./examples/webpack.config.js",
    "test": "ava --watch",
    "testonce": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickjohnson-dev/functional-forms.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nickjohnson-dev/functional-forms/issues"
  },
  "homepage": "https://github.com/nickjohnson-dev/functional-forms#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "lodash": "4.17.11",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-hyperscript": "3.0.0",
    "react-style-proptype": "2.0.0"
  },
  "devDependencies": {
    "ava": "0.18.1",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-class-properties": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-es2017": "6.22.0",
    "babel-preset-stage-3": "6.22.0",
    "babel-register": "6.22.0",
    "css-loader": "^0.28.4",
    "enzyme": "2.7.1",
    "eslint": "3.15.0",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-lodash-fp": "2.1.3",
    "html-webpack-plugin": "^2.8.1",
    "react-addons-test-utils": "15.4.2",
    "rimraf": "2.5.1",
    "sinon": "2.0.0-pre.5",
    "style-loader": "^0.18.2",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.9"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
